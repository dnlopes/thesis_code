<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

    <display-name>TPCW</display-name>

    <servlet>
      <servlet-name>TPCW_home_interaction.java</servlet-name>
      <servlet-class>TPCW_home_interaction.java</servlet-class>
    </servlet>

    <servlet>
      <servlet-name>TPCW_say_hello.java</servlet-name>
      <servlet-class>TPCW_say_hello.java</servlet-class>
    </servlet>



    <session-config>
      <session-timeout>30</session-timeout>    <!-- 30 minutes -->
    </session-config>

</web-app>
